function presenceChecker2(i)
    if i == 3 then
        deckTable = deck.cut(10)
    elseif i == 2 then
        deckTable = deck.cut(9)
    elseif i == 1 then
        deckTable = deck.cut(8)
    elseif i == 0 then
        deckTable = deck.cut(7)
    end
end

function presenceChecker3(i)
    if i == 2 then
        deckTable = deck.cut(9)
    elseif i == 1 then
        deckTable = deck.cut(8)
    elseif i == 0 then
        deckTable = deck.cut(7)
    end
end

function presenceChecker4(i)
    if i == 1 then
        deckTable = deck.cut(8)
    elseif i == 0 then
        deckTable = deck.cut(7)
    end
