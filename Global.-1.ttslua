function wordErrorChecking(user, word, language)
        errorCheck = 1
        if word == nil then
            errorCheck=-1
            return errorCheck
        end
        local word = string.lower(word)
        letterCheck = string.match(word, "j")
        local color = user.color
        if language == "English" then
            if letterCheck == "j" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "q")
            if letterCheck == "q" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "v")
            if letterCheck == "v" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "x")
            if letterCheck == "x" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "z")
            if letterCheck == "z" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
        end
        if language == "German" then
            if letterCheck == "j" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "q")
            if letterCheck == "q" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "v")
            if letterCheck == "v" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "x")
            if letterCheck == "x" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
            letterCheck = string.match(word, "z")
            if letterCheck == "y" then
                displayCheckWordFromColor(color)
                errorCheck = -1
            end
        end
        UI.setAttribute("wordFrom" .. color, "text", word)

        return errorCheck
end
function displayCheckWordFromColor(color)
    UI.setAttribute("wordFrom" .. color, "text", "nope.")
end
function hideWordBox(color)
    UI.setAttribute("wordPanel" .. color, "active", "false")
end
function pickWordsUI(player)
    hideWordBox(player.color)
end
function pickWords(user, color)
    if UI.getAttribute("wordPanel".. color, "active") == "false" then
        UI.setAttribute("wordPanel" .. color, "active", "true")
    end
end
function wordConfirm(user)
    word = UI.getAttribute("wordFrom" .. user.color, "text")
    errorCode = wordErrorChecking(user, word, language)
    if errorCode == 1 then
        if string.len(word) == 5 then
            pickWordsUI(user)
            createCards(string.sub(word, 1, 1), user.color, 1)
            createCards(string.sub(word, 2, 2), user.color, 2)
            createCards(string.sub(word, 3, 3), user.color, 3)
            createCards(string.sub(word, 4, 4), user.color, 4)
            createCards(string.sub(word, 5, 5), user.color, 5)
        else
            UI.setAttribute("wordFrom" .. user.color, "text", "")
            UI.setAttribute("wordFrom" .. user.color, "placeholder", "five letters")
        end
    else
        UI.setAttribute("wordFrom" .. user.color, "text", "")
        UI.setAttribute("wordFrom" .. user.color, "placeholder", "Nope.")
    end
end
function createCards(index, color, number)
    newObj = spawnObject({type = "card"})
    newObj.setCustomObject({
      face= card_faces[index],
      back= cardBack

    })
    newObj.setScale({0.40, 1.00, 0.40})
    if Player["White"].color == color then
        newObj.setPosition({-3.00, 3.00, -10.19})
        newObj.setRotation({0.00, 180.00, 180.00})
        wordWhite[number] = newObj.getGUID()
    end
    if Player["Red"].color == color then
        newObj.setPosition({-9.72, 3.00, -3.00})
        newObj.setRotation({0.00, 240.00, 180.00})
        wordRed[number] = newObj.getGUID()

    end
    if Player["Yellow"].color == color then
        newObj.setPosition({-7.42, 3.00, 6.96})
        newObj.setRotation({0.00, 300.00, 180.00})
        wordYellow[number] = newObj.getGUID()

    end
    if Player["Green"].color == color then
        newObj.setPosition({3.00, 3.00, 10.00})
        newObj.setRotation({0.00, 0.00, 180.00})
        wordGreen[number] = newObj.getGUID()
    end
    if Player["Blue"].color == color then
        newObj.setPosition({10.18, 3.00, 1.98})
        newObj.setRotation({0.00, 60.00, 180.00})
        wordBlue[number] = newObj.getGUID()
        print(wordBlue[number])
    end
    if Player["Pink"].color == color then
        newObj.setPosition({7.00, 3.00, -7.50})
        newObj.setRotation({0.00, 120.00, 180.00})
        wordPink[number] = newObj.getGUID()
    end
end



