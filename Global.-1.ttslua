function toggleDeductionSheet(user)
    local color = user.color
    active = UI.getAttribute(color .. "DeductionSheet", "active")
    if active == "false" then
        UI.setAttribute(color .."DeductionSheet", "active", "true")
    elseif active == "true" then
        UI.setAttribute(color .."DeductionSheet", "active", "false")
    end
end

