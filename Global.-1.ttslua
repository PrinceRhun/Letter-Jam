    toggleDeductionSheet(Player["White"])
end



function redProgress()
    if setup == 1 and Player["Red"].seated then
        if redLettersFlipped == 0 then
            redLetter1.flip()
            redIndicator1.flip()
            redLetter2.flip()
        end
        if redLettersFlipped == 1 then
            redLetter2.flip()
            redIndicator2.flip()
            redLetter3.flip()
        end
        if redLettersFlipped == 2 then
            redLetter3.flip()
            redIndicator3.flip()
            redLetter4.flip()
        end
        if redLettersFlipped == 3 then
            redLetter4.flip()
            redIndicator4.flip()
            redLetter5.flip()
        end
        if redLettersFlipped == 4 then
            redLetter5.flip()
